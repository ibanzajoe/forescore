  $(document).ready(function() {
    var $checkmark = "<img src='/work/forescout_checkbox'/>";
    $('.check_box, .value_bar').on('click', function(e){
      var $this = $(this)
      var key = $this.data('key')
      user_data[key] = [];
      var val = $this.data('value')
      var trigger = $this.data('trigger')
      var single = $this.data('single')

      if (trigger) {
        $this.closest('.slide').find('.option').fadeOut()
        $this.closest('.slide').find('.' + trigger).fadeIn()
      }

      if (single) {
        $this.closest('.slide').find('.checked').removeClass('checked')
      }

      if ($this.hasClass('checked')) {
        if (single) {
          $this.closest('.slide').find('.checked').removeClass('checked')
        }
        $this.removeClass('checked')
        user_data[key] = false;
        console.log(user_data, key, val);

      }else{
        $this.addClass('checked')
        user_data[key] = val;
        console.log("look at this: ", user_data, key, val);
      }
    })
    $(".name_input_field input").on("change", function (e){
      var $this = $(this)
      var key = $this.data('key')
      user_data[key] = $this.val();
      console.log(user_data[key]);
    })

    $('.next').on('click', function(){
      var skip_validate = $(this).data('skip-validate')

      if (!skip_validate) {
        var checks = $(this).closest('.slide').find('.checked')
        if (checks.length == 0){
          $(this).closest('.slide').find('.validation-error').css('visibility', 'visible')
          return false
        }else{
          $(this).closest('.slide').find('.validation-error').css('visibility', 'hidden')
        }
      }

      index++;
      slide(index)
    })

    $('.prev').on('click', function(){
      index--;
      slide(index)
    })

  })

  function slide(slide_index){
    var height = $('.slide:first').outerHeight()

    var slide_to = -(height * slide_index)
    $('#slideshow').css('transform', "matrix(1, 0, 0, 1, 0," + slide_to + ")")
  }

  $('.path-selector').on('change', function(e){
    var value = $(this).val()
    var key = $(this).attr('data-key');
    user_data[key] = value;
    console.log(key, user_data);

