<script>
  $(document).ready(function() {
    var $checkmark = "<img src='/work/forescout_checkbox'/>";
    $('.check_box').on('click', function(e){
      if ($(this).hasClass('checked')) {
        $(this).removeClass('checked')
      }else{
        $(this).addClass('checked')
      }
    })

    //* Empty checkbox to Checked checkbox
    $('.check_box_container_image').on("click", function (e){
      e.preventDefault();
      var x = $(this).attr('src');
      var slide = $(this).parents('.slide').attr('value');
      var slide_s = "." + slide;
      var img_class_s = slide_s + "_image";
      var input_imgs = $(img_class_s);
      if (x == '/work/forescout_rectangle.png') {
        for (var i = 0; i < input_imgs.length; i++) {
          if ($(input_imgs[i]).attr('src') == '/work/forescout_checkbox.png') {

            $(input_imgs[i]).attr('src', '/work/forescout_rectangle.png')
          }
        }
        $(this).attr('src', '/work/forescout_checkbox.png')

      }
      else {
        $(this).attr('src', '/work/forescout_rectangle.png')

      }
    })

    $('.empty_check').on("click", function (e){
      e.preventDefault();
      var y = $(this).attr('src');
      if (y == '/work/forescout_rectangle_empty.png') {
        $(this).attr('src', '/work/forescout_rectangle_checked.png')
      }
      else {
        $(this).attr('src', '/work/forescout_rectangle_empty.png')
      }
    })
  })

  var init = 0;
  showDivs(init);

  function plusDivs(n, y) {
    var height = $('.slide:first').height()
    console.log("this is the height: ", height)
    showDivs(height * n, y);
  }

  function showDivs(n, y) {
    var i = 0;
    var x = document.getElementById("slideshow");
    x.style.transform = "matrix(1, 0, 0, 1, 0," + n + ")";
  }

</script>

